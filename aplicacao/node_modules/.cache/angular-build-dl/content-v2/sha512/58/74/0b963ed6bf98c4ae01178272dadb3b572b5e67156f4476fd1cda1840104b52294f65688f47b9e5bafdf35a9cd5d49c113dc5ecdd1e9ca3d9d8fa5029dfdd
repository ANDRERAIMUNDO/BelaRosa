onse=>\n        {\n          this.registro = response as RegistroDTO;\n        },\n       catchError =>\n       {\n         if (catchError.status == 403)\n         {\n           this.router.navigate(['/dashboard/cart']);\n         }\n       });\n    }\n  }\n  removeItem(produto: ProdutoDTO) {\n    this.itens = this.cardService.removerProduto(produto).itens;\n  }\n  addQuantidade(produto: ProdutoDTO) {\n    this.itens = this.cardService.addQuantidade(produto).itens;\n  }\n  removerQuantidade(produto: ProdutoDTO) {\n    this.itens = this.cardService.removerQuantidade(produto).itens;\n  }\n  total(): number {\n    return this.cardService.total();\n  }\n\n  loadImage() {\n    for (var i =0; i<this.itens.length; i++)\n    {\n      let itens = this.itens[i];\n      this.produtoService.getSmallImage(itens.produto.id)\n      .subscribe(response=>\n        {\n          itens.produto.imageUrl = `${API_CONFIG.bukectBaseUrl}/produtos/prod${itens.produto.id}-small.png`;\n        }, catchError => \n        {\n          if (catchError.status == 0)\n          {\n            itens.produto.imageUrl = '../../assets/Camera.svg';\n          }\n        });\n    }\n  }\n\n  navigate  () {\n    this.router.navigate(['/dashboard/circle-adress']);\n  }\n}","export default \"#container {\\n  text-align: center;\\n  position: absolute;\\n  left: 0;\\n  right: 0;\\n  top: 60%;\\n  transform: translateY(-50%);\\n}\\n\\n#container-no-cart {\\n  text-align: center;\\n  position: absolute;\\n  left: 0;\\n  right: 0;\\n  top: 40%;\\n  transform: translateY(-50%);\\n}\\n\\n#ion-icon-nolinebreak-remove {\\n  width: 40px;\\n  height: 40px;\\n  font-size: 100px;\\n  color: #bd1198;\\n}\\n\\n#ion-icon-nolinebreak-add {\\n  width: 40px;\\n  height: 40px;\\n  font-size: 100px;\\n  color: #023d14;\\n}\\n\\nion-header {\\n  font-family: \\\"Lucida Sans Regular\\\";\\n  font-size: x-small;\\n  color: #080808;\\n  text-align: center;\\n  --ion-background-color: rgb(240, 191, 233);\\n}\\n\\nion-content {\\n  --