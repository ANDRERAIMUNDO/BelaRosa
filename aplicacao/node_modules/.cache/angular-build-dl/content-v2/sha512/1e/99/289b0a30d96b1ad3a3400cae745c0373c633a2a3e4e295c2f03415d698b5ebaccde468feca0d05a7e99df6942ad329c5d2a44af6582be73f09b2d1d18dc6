        let cep = this.formGroupAddress.get('cep').value;
        this.clienteService.viaCep(cep)
            .subscribe(response => {
            this.endereco = response;
        }, catchError => {
            this.presentEnderecoConfirm();
        });
    }
    loandRegistro() {
        this.RegistropresentLoading();
        this.registroService.findEmailUpdateDates(this.email)
            .subscribe(response => {
