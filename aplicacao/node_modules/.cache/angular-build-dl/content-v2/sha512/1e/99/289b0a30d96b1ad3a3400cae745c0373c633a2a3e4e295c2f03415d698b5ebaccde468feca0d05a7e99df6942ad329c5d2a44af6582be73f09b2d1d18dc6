            password: ['', _angular_forms__WEBPACK_IMPORTED_MODULE_8__.Validators.minLength(5)]
        });
    }
    showPassword() {
        let show = "text";
        let hide = "password";
        this.senha = show;
        setTimeout(() => {
            this.senha = hide;
        }, 3000);
    }
    updateRegistroPasswordForm(registroform) {
        this.checkPassword();
        if (this.checkPasswordValue == true) {
            this.registroService.updatePassword(this.registro.id, registroform)
                .subscribe(response => {
                this.loandRegistro();
            });
        }
    }
    formCliente() {
        this.id = this.route.snapshot.params['id'];
        this.formGroupCliente = this.formBuilder.group({
            phone: [null, [_angular_forms__WEBPACK_IMPORTED_MODULE_8__.Validators.required, _angular_forms__WEBPACK_IMPORTED_MODULE_8__.Validators.minLength(11)]]
        });
    }
    updateClienteForm(clienteform) {
        this.clienteService.update(this.cliente.id, clienteform)
            .subscribe(response => {
            this.loandCliente();
        }, catchError => {
            this.erroSuccess();
            this.router.navigate(['/dashboard/profile']);
        });
    }
    formAddress() {
        this.id = this.route.snapshot.params['id'];
        this.formGroupAddress = this.formBuilder.group({
            cep: ['', [_angular_forms__WEBPACK_IMPORTED_MODULE_8__.Validators.required]],
