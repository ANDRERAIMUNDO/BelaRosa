            this.erroSuccess();
            this.router.navigate(['/dashboard/profile']);
        });
    }
    updateOnRegistro() {
        this.updateRegistroValue = true;
    }
    updateOnCliente() {
        this.updateClienteValue = true;
    }
    updateOnAddress() {
        this.updateAddressValue = true;
    }
    checkEmail() {
        this.email = this.formGroupRegistroEmail.get('email').value;
        this.emailConfirm = this.formGroupRegistroEmailConfirm.get('emailConfirm').value;
        let email = this.email;
        let emailConfirm = this.emailConfirm;
        if (email == emailConfirm) {
            this.checkEmailValue = true;
        }
        else {
            this.presentEmailConfirm();
        }
    }
    checkPassword() {
        this.password = this.formGroupRegistroPassword.get('password').value;
        this.passwordConfirm = this.formGroupRegistroPasswordConfirm.get('passwordConfirm').value;
        let password = this.password;
        let passwordConfirm = this.passwordConfirm;
        if (password == passwordConfirm) {
            this.checkPasswordValue = true;
        }
        else {
            this.presentErrorPasswordConfirm();
        }
    }
