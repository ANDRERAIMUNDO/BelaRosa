{ AuthService } from '../services/auth.service';\nimport { CategoriaService } from '../services/categoria.service';\n\n@Component({\n  selector: 'app-login',\n  templateUrl: './login.page.html',\n  styleUrls: ['./login.page.scss'],\n})\nexport class LoginPage implements OnInit {\n  \n  credenciais: CredenciaisDTO = {\n    email: \"\",\n    password: \"\"\n  };\n\n  constructor(\n    public router: Router,\n    public authService: AuthService,\n    public loadingController: LoadingController) { }\n\n  ngOnInit() {\n    this.refresh();\n  }\n\n  refresh() {\n    this.authService.refreshToken()\n    .subscribe(response=>\n      {\n        this.authService.sucessLogin(response.headers.get('Authorization'));\n        this.router.navigate(['/dashboard/index']);\n      },\n      catchError=>\n      {\n        this.router.navigate(['/login']);\n      });\n  }\n\n  login(){\n    this.loand();\n    this.authService.authetocation(this.credenciais)\n    .subscribe(response=> \n      {\n      this.authService.sucessLogin(response.headers.get('Authorization'));\n      this.router.navigate(['/dashboard/index']);\n      }, \n      catchError=>\n      {\n        if (catchError.status == 401)\n      {\n        this.router.navigate(['/login']);\n      }\n    });\n  }\n\n  loand(){\n    this.presentLoading();\n  }\n\n  async presentLoading() {\n    const loading = await this.loadingController.create({\n      cssClass: 'my-custom-class',\n      message: 'Aguarde...',\n      keyboardClose: true,\n      spinner:\"lines-small\",\n      animated: true,\n      duration: 3000,\n    });\n    await loading.present();\n    const { role, data } = await loading.onDidDismiss();\n    console.log('Loading dismissed!');\n  }\n\n}\n","export default \".content {\\n  --ion-background-color: rgb(243, 156, 231);\\n}\\n\\n.ion-padding-top {\\n  padding-top: 50 px;\\n}\\n\\n.ion-text-center {\\n  text-align: center;\\n}\\n\\n.ion-padding-top {\\n  padding-top: 50 px;\\n}\\n\\n.ion-padding {\\n  padding: 30px;\\n}\\n\\nion-header {\\n  --ion-background-color: rgb(231, 163, 203);\\n}\\n\\nh5 {\\n  font-family: \\\"sans-serif\\\";\\n  font-size: medium;\\n  font-weight: bolder;\\n  font-style: italic;\\n  color: white;\\n  text-align: left;\\n}\\n\\nion-buttons {\\n  color: white;\\n}\\n\\nion-input {\\n  font-family: \\\"sans-serif\\\";\\n  font-style: italic;\\n  color: white;\\n  font-size: small;\\n}\\n\\nion-button {\\n  color: #f0bfe9;\\n}\\n\\n.my-custom-class {\\n  color: #e7490a;\\n  font-size: xx-large;\\n}\\n/*# sourceMappingURL=data:application/json;base64, */\";","export default \"<ion-header>\\n  <ion-toolbar>\\n    <ion-buttons slot=\\\"start\\\">\\n      <ion-back-button>back</ion-back-button>\\n    </ion-buttons>\\n    <ion-title><h5>Retornar</h5></ion-title>\\n  </ion-toolbar>\\n</ion-header>\\n\\n<ion-content class=\\\"content\\\"[fullscreen]=\\\"true\\\">\\n  <br>\\n  <br>\\n  <br>\\n  <br>\\n  <br>\\n  <br>\\n  <br>\\n  <br>\\n<div class=\\\"ion-padding-top\\\">\\n<div class=\\\"ion-padding\\\">\\n<div class=\\\"ion-text-center\\\"> \\n  <form>\\n    <div class=\\\"form-group\\\">\\n      <ion-item lines=\\\"full\\\">\\n        <ion-label position=\\\"floating\\\">Email</ion-label>\\n        <ion-input type=\\\"text\\\" class=\\\"form-control\\\" id=\\\"email\\\" required [(ngModel)]=\\\"credenciais.email\\\" name=\\\"email\\\"> </ion-input>\\n      </ion-item>\\n    </div>\\n    <div class=\\\"form-group\\\">\\n      <ion-item lines=\\\"full\\\">\\n        <ion-label position=\\\"floating\\\">Password</ion-label>\\n        <ion-input type=\\\"password\\\" class=\\\"form-control\\\" id=\\\"password\\\" required [(ngModel)]=\\\"credenciais.password\\\" name=\\\"password\\\"></ion-input>\\n      </ion-item>\\n    </div>\\n    <ion-row>\\n      <ion-col>\\n        <ion-button type=\\\"submit\\\" color=\\\"light\\\" expand=\\\"block\\\" (click)=\\\"login()\\\">Entrar</ion-button>\\n        <a [routerLink]=\\\"['/forgot-password']\\\" class=\\\"small-text\\\">Recupera sua senha?</a>\\n      </ion-col>\\n    </ion-row>\\n  </form>\\n</div>\\n</div>\\n</div>\\n</ion-content>\";"],"sourceRoot":"webpack:///"}